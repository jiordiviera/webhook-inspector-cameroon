{
  "name": "genuka-webhook-inspector-v2",
  "version": "2.0.0",
  "description": "Inspector professionnel pour webhooks Genuka API avec Express + React + Bun",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run client:dev\" \"bun run server:dev\"",
    "server:dev": "node --watch server/index.js",
    "client:dev": "cd client && bun run dev",
    "build": "cd client && bun run build",
    "start": "concurrently \"bun run server:start\" \"bun run client:start\"",
    "server:start": "node server/index.js",
    "client:start": "cd client && bun run start",
    "test": "bun test",
    "setup": "bun install && cd client && bun install"
  },
  "keywords": ["genuka", "webhook", "cameroon", "express", "react", "sqlite", "bun"],
  "author": "Genuka Team ðŸ‡¨ðŸ‡²",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.6",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "trustedDependencies": ["sqlite3"]
}