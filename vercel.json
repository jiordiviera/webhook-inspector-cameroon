{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/server"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}