{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/bun"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/src/server.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server.ts"
    },
    {
      "src": "/ws",
      "dest": "/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ],
  "functions": {
    "src/server.ts": {
      "runtime": "bun@latest"
    }
  },
  "env": {
    "PORT": "3000"
  }
}